{
   "api_base_url" : "http://github.com/api/v2/",
   "version" : "0.1",
   "methods" : {
      "user_information" : {
         "required" : [
            "username",
            "format"
         ],
         "path" : "/:format/user/show/:username",
         "method" : "GET"
      },
      "user_following" : {
         "required" : [
            "user",
            "format"
         ],
         "path" : "/:format/user/show/:user/followers",
         "method" : "GET"
      }
   },
   "name" : "GitHub",
   "author" : [
      "franck cuny <franck@lumberjaph.net>"
   ],
   "meta" : {
      "documentation" : "http://develop.github.com/"
   }
}
