      "home_timeline" : {
         "params" : [
            "since_id",
            "max_id",
            "count",
            "page",
            "trim_user",
            "include_entities"
         ],
         "required" : [
            "format"
         ],
         "path" : "/statuses/home_timeline.:format",
         "method" : "GET",
         "authentication" : "1"
