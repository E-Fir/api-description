   "api_format" : [
      "json",
      "rss",
      "xml",
      "atom"
   ],
   "name" : "Twitter",
   "author" : [
      "franck cuny <franck@lumberjaph.net>"
   ],
   "meta" : {
      "documentation" : "http://dev.twitter.com/"
   }
